<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pantallas.Agenda.AgendaFragment">


    <TextView
        android:id="@+id/elegirFecha"
        android:layout_width="183dp"
        android:layout_height="28dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/eligirFecha"
        android:textAlignment="center"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buscarEventos"
        android:layout_width="37dp"
        android:layout_height="28dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="21dp"
        android:background="@drawable/buscador"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/nuevoEvento"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/nuevoEvento"
        android:layout_width="37dp"
        android:layout_height="28dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="68dp"
        android:background="@drawable/nuevo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/eventos"
        android:layout_width="338dp"
        android:layout_height="469dp"
        android:layout_marginTop="62dp"
        app:layout_constraintTop_toBottomOf="@+id/elegirFecha"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp" />


</androidx.constraintlayout.widget.ConstraintLayout>